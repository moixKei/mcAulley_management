package com.mcaulley.app.service;

import com.mcaulley.app.entity.Alumna;
import java.time.LocalDate;
import java.util.List;
import java.util.Map;

public interface AlumnaService {
	
	List<Alumna> buscarConFiltrosAvanzados(String nombre, String apellido, LocalDate fechaDesde, LocalDate fechaHasta);
    Map<String, Long> obtenerEstadisticasRegistro();
    void desactivarAlumnasAntiguas(LocalDate fechaLimite);
}
